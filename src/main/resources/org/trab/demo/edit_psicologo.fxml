<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="661.0" prefWidth="936.0" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.trab.demo.controller.EditPsicologoController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <items>
          <Button fx:id="btn_dash" mnemonicParsing="false" onAction="#telaDashPsicologo" text="Início">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btn_agenda" mnemonicParsing="false" onAction="#telaAgenda" text="Agenda">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btn_cadHorarios" mnemonicParsing="false" onAction="#telaCadHorarios" text="Cadastrar Horários">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btn_historicoConsultas" mnemonicParsing="false" onAction="#telaHistoricoConsultas" text="Histórico de Consultas">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btn_perfil" mnemonicParsing="false" text="Perfil">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
        </items>
         <padding>
            <Insets bottom="20.0" left="20.0" top="20.0" />
         </padding>
      </ToolBar>
      <Label text="Nome:">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="40.0" right="40.0" top="40.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tf_nome" prefHeight="34.0" prefWidth="782.0">
         <VBox.margin>
            <Insets bottom="30.0" left="40.0" right="40.0" />
         </VBox.margin>
      </TextField>
      <Label text="Telefone:">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="40.0" right="40.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tf_telefone" prefHeight="34.0" prefWidth="570.0">
         <VBox.margin>
            <Insets bottom="30.0" left="40.0" right="40.0" />
         </VBox.margin>
      </TextField>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <VBox prefHeight="100.0" prefWidth="368.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label prefHeight="27.0" prefWidth="59.0" text="CRP:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_crp" prefHeight="34.0" prefWidth="570.0" />
               </children>
            </VBox>
            <VBox prefHeight="100.0" prefWidth="428.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label prefHeight="27.0" prefWidth="59.0" text="CPF:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_cpf" prefHeight="34.0" prefWidth="570.0" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets left="40.0" right="40.0" />
         </VBox.margin>
      </HBox>
      <Label prefHeight="27.0" prefWidth="82.0" text="E-mail:">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="40.0" right="40.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tf_email" prefHeight="34.0" prefWidth="570.0">
         <VBox.margin>
            <Insets bottom="30.0" left="40.0" right="40.0" />
         </VBox.margin>
      </TextField>
      <Label prefHeight="27.0" prefWidth="82.0" text="Senha:">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="40.0" right="40.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tf_senha" prefHeight="34.0" prefWidth="570.0">
         <VBox.margin>
            <Insets left="40.0" right="40.0" />
         </VBox.margin></TextField>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="944.0">
         <children>
            <Button fx:id="btn_salvar" mnemonicParsing="false" text="Salvar">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
