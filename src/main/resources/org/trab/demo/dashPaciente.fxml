<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.trab.demo.controller.DashboardPaciente">
    <children>
        <AnchorPane prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="lbNome1" prefHeight="18.0" prefWidth="200.0" text="Seja bem vindo," AnchorPane.leftAnchor="27.0" AnchorPane.topAnchor="49.0">
                    <font><Font size="18.0" /></font>
                </Label>
                <Label fx:id="lbNome" prefHeight="18.0" prefWidth="200.0" text="Nome" AnchorPane.leftAnchor="27.0" AnchorPane.topAnchor="73.0">
                    <font><Font size="18.0" /></font>
                </Label>
                <Label fx:id="lbProxConsulta" prefHeight="26.0" prefWidth="200.0" text="Proxima(s) consulta(s):" AnchorPane.leftAnchor="27.0" AnchorPane.topAnchor="130.0">
                    <font><Font size="16.0" /></font>
                </Label>
                <Button fx:id="btnAcessarPerfil" mnemonicParsing="false" onAction="#telaPerfil" prefHeight="30.0" prefWidth="170.0" text="Acessar Perfil" AnchorPane.rightAnchor="27.0" AnchorPane.topAnchor="52.0" />
                <Button fx:id="btnAgendar" mnemonicParsing="false" onAction="#telaAgendamento" prefHeight="30.0" prefWidth="170.0" text="Agendar Consultas" AnchorPane.rightAnchor="27.0" AnchorPane.topAnchor="127.0" />
                <Button fx:id="btnRemarcar" mnemonicParsing="false" onAction="#telaRemarcacao" prefHeight="30.0" prefWidth="170.0" text="Remarcar Consultas" AnchorPane.rightAnchor="27.0" AnchorPane.topAnchor="169.0" />
                <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#telaCancelamento" prefHeight="30.0" prefWidth="170.0" text="Cancelar Consultas" AnchorPane.rightAnchor="27.0" AnchorPane.topAnchor="219.0" />

                <HBox prefHeight="213.0" prefWidth="410.0" AnchorPane.bottomAnchor="27.0" AnchorPane.leftAnchor="27.0" AnchorPane.rightAnchor="203.0" AnchorPane.topAnchor="160.0">
                    <children>
                        <TableView fx:id="tvConsultas" HBox.hgrow="ALWAYS">
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                            <columns>
                                <TableColumn fx:id="colDia" prefWidth="205.6" text="Dia" />
                                <TableColumn fx:id="colHora" prefWidth="199.2" text="Hora" />
                            </columns>
                        </TableView>
                    </children>
                </HBox>

                <Button fx:id="btnDeslogar" mnemonicParsing="false" onAction="#deslogar" prefHeight="30.0" prefWidth="170.0" text="Deslogar" AnchorPane.bottomAnchor="27.0" AnchorPane.rightAnchor="27.0">
                    <font><Font size="14.0" /></font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>
